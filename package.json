{
  "name": "@goblindegook/gatsby-starter-typescript",
  "description": "A Gatsby starter the way I like it.",
  "version": "1.0.0",
  "author": "Lu√≠s Rodrigues <hello@goblindegook.com>",
  "dependencies": {
    "@emotion/core": "^10.0.4",
    "@emotion/styled": "^10.0.4",
    "@reach/router": "^1.2.0",
    "@types/lunr": "^2.1.6",
    "emotion": "^10.0.4",
    "emotion-server": "^10.0.4",
    "gatsby": "^2.0.7",
    "gatsby-plugin-catch-links": "^2.0.2",
    "gatsby-plugin-emotion": "^3.0.1",
    "gatsby-plugin-feed": "^2.0.5",
    "gatsby-plugin-lunr": "^1.2.0",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.1",
    "gray-percentage": "^2.0.0",
    "lodash": "^4.17.5",
    "lunr": "^2.3.3",
    "normalize.css": "^8.0.0",
    "prismjs": "^1.14.0",
    "ramda": "^0.26.1",
    "react": "^16.5.2",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typeface-domine": "^0.0.54",
    "typeface-montserrat": "^0.0.54",
    "typography": "^0.16.17",
    "typography-breakpoint-constants": "^0.16.18"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.104",
    "@types/node": "^10.1.3",
    "@types/ramda": "^0.25.20",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.5",
    "@types/typography": "^0.16.0",
    "core-js": "^2.6.2",
    "husky": "^1.0.0-rc.15",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.0.1",
    "jest-dom": "^3.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.11.1",
    "react-testing-library": "^5.1.0",
    "ts-jest": "^23.10.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1"
  },
  "keywords": [
    "gatsby",
    "markdown",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write '{__mocks__,src,test}/**/*.{ts,tsx}' '*.js'",
    "prebuild": "yarn lint && yarn test",
    "precommit": "lint-staged",
    "lint": "tslint --project tsconfig.json '{src,test}/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --project tsconfig.json",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setup-jest.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
